#include <iostream>
using namespace std;

int main() {
    long long int size,k;
    cin>>k;
    while(k--){
        long long int sum=0,a[size];
        cin>>size;
        for(int i=0;i<size;i++)
        cin>>a[i];
        for(int i=0;i<size;i++){
            if(a[i]==-1){
                a[i]=sum/(i);
            }
            cout<<a[i]<<" ";
            sum+=a[i];
        }
        cout<<"\n";
    }

	
	return 0;
}
